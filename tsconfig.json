{
  "compilerOptions": {
    "target": "ES2024",
    "module": "ESNext",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
  },
  "include": [
    "src/**/*"
    ,"globals/globals.d.ts"],
  "exclude": [
    "node_modules"
  ]
}