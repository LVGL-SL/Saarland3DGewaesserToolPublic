{
  "name": "plugin-name",
  "main": "src/index.ts",
  "scripts": {
    "start": "npm run build",
    "dev": "nodemon --watch src --ext js,ts,vue,json,css,html,scss,sass,less,md,txt,xml,yaml,yml --delay 2.5 --exec \"npm run start\"",
    "build": "tsc --rootDir src --outDir build && copyfiles -u 1 --exclude \"**/*.ts\" \"src/**/*\" build/ && npm run serve",
    "serve":"vcmplugin serve --vcm ../wega3d -e ./build/index.js"
  },
  "devDependencies": {
    "@vcmap/plugin-cli": "1.0.0",
    "@vue/compiler-sfc": "^3.5.16",
    "chokidar-cli": "^3.0.0",
    "copyfiles": "^2.4.1",
    "file-loader": "^6.2.0",
    "nodemon": "^3.1.10",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3",
    "vue-loader": "^17.4.2",
    "webpack": "^5.99.9",
    "webpack-dev-middleware": "^7.4.2"
  },
  "dependencies": {
    "@vcmap/cesium": "^1.97.1",
    "cesium": "^1.79.1",
    "ol": "^10.5.0"
  }
}
